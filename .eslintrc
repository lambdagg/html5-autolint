{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
		"indent": ["warn", 4],
		"consistent-return": "off",
		"no-console": "off",
		"no-param-reassign": "off",
		"no-plusplus": "off"
    }
}
